<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>SynergyNetAppSystem.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>SynergyNetAppSystemService</servlet-name>
  	<servlet-class>synergynet3.web.appsystem.server.SynergyNetAppSystemServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SynergyNetAppSystemService</servlet-name>
  	<url-pattern>synergynet3.web.appsystem.SynergyNetAppSystem/SynergyNetAppSystemService</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>SynergyNetWebCommonsService</servlet-name>
  	<servlet-class>synergynet3.web.commons.server.SynergyNetWebCommonsServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SynergyNetWebCommonsService</servlet-name>
  	<url-pattern>synergynet3.web.appsystem.SynergyNetAppSystem/SynergyNetWebCommonsService</url-pattern>
  </servlet-mapping>

</web-app>
